    "angular-ui-router": "^0.3.1",
    "autoprefixer": "^6.0.1",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-runtime": "^6.11.6",
    "bootstrap": "^3.3.5",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
